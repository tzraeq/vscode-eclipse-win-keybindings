{
    "name": "vscode-eclipse-win-keybindings",
    "displayName": "Eclipse Win Keymap",
    "description": "Eclipse win keybindings for Visual Studio Code",
    "version": "0.20.0",
    "publisher": "tzraeq",
    "engines": {
        "vscode": "^1.6.0"
    },
    "categories": [
        "Keymaps"
    ],
    "keywords": [
        "keybinding",
        "keymap",
        "eclipse",
        "win",
        "windows",
        "shortcut"
    ],
    "icon": "vscode-eclipse-keybindings.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/tzraeq/vscode-eclipse-win-keybindings.git"
    },
    "homepage": "https://github.com/tzraeq",
    "bugs": {
        "url": "https://github.com/tzraeq/vscode-eclipse-win-keybindings/issues"
    },
    "contributes": {
        "keybindings": [
            {
                "key": "ctrl+c",
                "command": "editor.action.clipboardCopyAction",
                "when": "textInputFocus"
            },
            {
                "key": "ctrl+c",
                "command": "workbench.action.terminal.copySelection",
                "when": "terminalFocus && terminalTextSelected"
            },
            {
                "key": "ctrl+c",
                "command": "execCopy"
            },
            {
                "key": "ctrl+c",
                "command": "keybindings.editor.copyKeybindingEntry",
                "when": "inKeybindings && keybindingFocus"
            },
            {
                "key": "ctrl+c",
                "command": "editor.action.webvieweditor.copy",
                "when": "!editorFocus && !inputFocus && activeEditor == 'WebviewEditor'"
            },
            {
                "key": "ctrl+c",
                "command": "filesExplorer.copy",
                "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
            },
            {
                "key": "ctrl+v",
                "command": "execPaste"
            },
            {
                "key": "ctrl+v",
                "command": "editor.action.clipboardPasteAction",
                "when": "textInputFocus && !editorReadonly"
            },
            {
                "key": "ctrl+v",
                "command": "editor.action.webvieweditor.paste",
                "when": "!editorFocus && !inputFocus && activeEditor == 'WebviewEditor'"
            },
            {
                "key": "ctrl+v",
                "command": "workbench.action.terminal.paste",
                "when": "terminalFocus"
            },
            {
                "key": "ctrl+v",
                "command": "filesExplorer.paste",
                "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"
            },
            {
                "key": "ctrl+a",
                "command": "workbench.action.terminal.selectAll",
                "when": "terminalFocus"
            },
            {
                "key": "ctrl+a",
                "command": "editor.action.selectAll",
                "when": "textInputFocus"
            },
            {
                "key": "ctrl+a",
                "command": "list.selectAll",
                "when": "listFocus && listSupportsMultiselect && !inputFocus"
            },
            {
                "key": "ctrl+a",
                "command": "editor.action.webvieweditor.selectAll",
                "when": "!editorFocus && !inputFocus && activeEditor == 'WebviewEditor'"
            },
            {
                "key": "ctrl+y",
                "command": "redo",
                "when": "textInputFocus && !editorReadonly"
            },
            {
                "key": "ctrl+z",
                "command": "undo",
                "when": "textInputFocus && !editorReadonly"
            },
            {
                "mac": "ctrl+3",
                "win": "ctrl+3",
                "linux": "ctrl+3",
                "key": "ctrl+3",
                "command": "workbench.action.showCommands"
            },
            {
                "mac": "ctrl+shift+r",
                "win": "ctrl+shift+r",
                "linux": "ctrl+shift+r",
                "key": "ctrl+shift+r",
                "command": "workbench.action.quickOpen"
            },
            {
                "mac": "ctrl+h",
                "win": "ctrl+h",
                "linux": "ctrl+h",
                "key": "ctrl+h",
                "command": "workbench.view.search"
            },
            {
                "mac": "ctrl+shift+t",
                "win": "ctrl+shift+t",
                "linux": "ctrl+shift+t",
                "key": "ctrl+shift+t",
                "command": "workbench.action.showAllSymbols"
            },
            {
                "mac": "ctrl+l",
                "win": "ctrl+l",
                "linux": "ctrl+l",
                "key": "ctrl+l",
                "command": "workbench.action.gotoLine"
            },
            {
                "mac": "ctrl+w",
                "win": "ctrl+w",
                "linux": "ctrl+w",
                "key": "ctrl+w",
                "command": "workbench.action.closeActiveEditor"
            },
            {
                "mac": "ctrl+shift+s",
                "win": "ctrl+shift+s",
                "linux": "ctrl+shift+s",
                "key": "ctrl+shift+s",
                "command": "workbench.action.files.saveAll"
            },
            {
                "mac": "ctrl+alt+s",
                "win": "ctrl+alt+s",
                "linux": "ctrl+alt+s",
                "key": "ctrl+alt+s",
                "command": "workbench.action.files.saveAs"
            },
            {
                "mac": "ctrl+alt+t",
                "win": "ctrl+alt+t",
                "linux": "ctrl+alt+t",
                "key": "ctrl+alt+t",
                "command": "workbench.action.terminal.toggleTerminal"
            },
            {
                "mac": "ctrl+m",
                "win": "ctrl+m",
                "linux": "ctrl+m",
                "key": "ctrl+m",
                "command": "workbench.action.toggleSidebarVisibility"
            },
            {
                "mac": "ctrl+e",
                "win": "ctrl+e",
                "linux": "ctrl+e",
                "key": "ctrl+e",
                "command": "workbench.action.openPreviousRecentlyUsedEditorInGroup"
            },
            {
                "mac": "ctrl+shift+w",
                "win": "ctrl+shift+w",
                "linux": "ctrl+shift+w",
                "key": "ctrl+shift+w",
                "command": "workbench.action.closeAllEditors"
            },
            {
                "mac": "ctrl+shift+f4",
                "win": "ctrl+shift+f4",
                "linux": "ctrl+shift+f4",
                "key": "ctrl+shift+f4",
                "command": "workbench.action.closeAllEditors"
            },
            {
                "mac": "f12",
                "win": "f12",
                "linux": "f12",
                "key": "f12",
                "command": "workbench.action.focusActiveEditorGroup"
            },
            {
                "mac": "ctrl+shift+c",
                "win": "ctrl+shift+c",
                "linux": "ctrl+shift+c",
                "key": "ctrl+shift+c",
                "command": "editor.action.commentLine",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+/",
                "win": "ctrl+shift+/",
                "linux": "ctrl+shift+/",
                "key": "ctrl+shift+/",
                "command": "editor.action.blockComment",
                "when": "editorTextFocus"
            },
            {
                "mac": "shift+alt+y",
                "win": "shift+alt+y",
                "linux": "shift+alt+y",
                "key": "shift+alt+y",
                "command": "editor.action.toggleWordWrap",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+alt+j",
                "win": "ctrl+alt+j",
                "linux": "ctrl+alt+j",
                "key": "ctrl+alt+j",
                "command": "editor.action.joinLines",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+o",
                "win": "ctrl+o",
                "linux": "ctrl+o",
                "key": "ctrl+o",
                "command": "workbench.action.gotoSymbol",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+f",
                "win": "ctrl+shift+f",
                "linux": "ctrl+shift+f",
                "key": "ctrl+shift+f",
                "command": "editor.action.formatSelection",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "mac": "ctrl+shift+f",
                "win": "ctrl+shift+f",
                "linux": "ctrl+shift+f",
                "key": "ctrl+shift+f",
                "command": "editor.action.formatDocument",
                "when": "editorTextFocus && !editorHasSelection"
            },
            {
                "key": "ctrl+alt+down",
                "command": "editor.action.copyLinesDownAction",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+alt+up",
                "command": "editor.action.copyLinesUpAction",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+down",
                "command": "editor.action.moveLinesDownAction",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+up",
                "command": "editor.action.moveLinesUpAction",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+d",
                "command": "editor.action.deleteLines",
                "when": "editorTextFocus"
            },
            {
                "key": "ctrl+shift+delete",
                "command": "deleteAllRight",
                "when": "editorTextFocus"
            },
            {
                "key": "alt+shift+r",
                "command": "editor.action.rename",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+o",
                "win": "ctrl+shift+o",
                "linux": "ctrl+shift+o",
                "key": "ctrl+shift+o",
                "command": "editor.action.organizeImports",
                "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)source\\.organizeImports\\b/"
            },
            {
                "mac": "ctrl+space",
                "win": "ctrl+space",
                "linux": "ctrl+space",
                "key": "ctrl+space",
                "command": "editor.action.triggerSuggest",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+.",
                "win": "ctrl+.",
                "linux": "ctrl+.",
                "key": "ctrl+.",
                "command": "editor.action.marker.next",
                "when": "editorFocus && !editorReadonly"
            },
            {
                "mac": "f3",
                "win": "f3",
                "linux": "f3",
                "key": "f3",
                "command": "editor.action.goToDeclaration",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+1",
                "win": "ctrl+1",
                "linux": "ctrl+1",
                "key": "ctrl+1",
                "command": "editor.action.quickFix",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+g",
                "win": "ctrl+shift+g",
                "linux": "ctrl+shift+g",
                "key": "ctrl+shift+g",
                "command": "editor.action.referenceSearch.trigger",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+b",
                "win": "ctrl+shift+b",
                "linux": "ctrl+shift+b",
                "key": "ctrl+shift+b",
                "command": "editor.debug.action.toggleBreakpoint",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+x",
                "win": "ctrl+shift+x",
                "linux": "ctrl+shift+x",
                "key": "ctrl+shift+x",
                "command": "editor.action.transformToUppercase",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+y",
                "win": "ctrl+shift+y",
                "linux": "ctrl+shift+y",
                "key": "ctrl+shift+y",
                "command": "editor.action.transformToLowercase",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+k",
                "win": "ctrl+k",
                "linux": "ctrl+k",
                "key": "ctrl+k",
                "command": "editor.action.nextSelectionMatchFindAction",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+shift+k",
                "win": "ctrl+shift+k",
                "linux": "ctrl+shift+k",
                "key": "ctrl+shift+k",
                "command": "editor.action.previousSelectionMatchFindAction",
                "when": "editorTextFocus"
            },
            {
                "mac": "ctrl+left",
                "win": "ctrl+left",
                "linux": "ctrl+left",
                "key": "ctrl+left",
                "command": "cursorWordPartLeft",
                "when": "textInputFocus"
            },
            {
                "mac": "ctrl+right",
                "win": "ctrl+right",
                "linux": "ctrl+right",
                "key": "ctrl+right",
                "command": "cursorWordPartRight",
                "when": "textInputFocus"
            },
            {
                "mac": "ctrl+shift+left",
                "win": "ctrl+shift+left",
                "linux": "ctrl+shift+left",
                "key": "ctrl+shift+left",
                "command": "cursorWordPartLeftSelect",
                "when": "textInputFocus"
            },
            {
                "mac": "ctrl+shift+right",
                "win": "ctrl+shift+right",
                "linux": "ctrl+shift+right",
                "key": "ctrl+shift+right",
                "command": "cursorWordPartRightSelect",
                "when": "textInputFocus"
            },
            {
                "key": "alt+left",
                "command": "workbench.action.navigateBack"
            },
            {
                "key": "alt+right",
                "command": "workbench.action.navigateForward"
            },
            {
                "mac": "f8",
                "win": "f8",
                "linux": "f8",
                "key": "f8",
                "command": "workbench.action.debug.continue",
                "when": "inDebugMode"
            },
            {
                "mac": "ctrl+f11",
                "win": "ctrl+f11",
                "linux": "ctrl+f11",
                "key": "ctrl+f11",
                "command": "workbench.action.debug.start",
                "when": "!inDebugMode"
            },
            {
                "mac": "f5",
                "win": "f5",
                "linux": "f5",
                "key": "f5",
                "command": "workbench.action.debug.stepInto",
                "when": "inDebugMode"
            },
            {
                "mac": "f7",
                "win": "f7",
                "linux": "f7",
                "key": "f7",
                "command": "workbench.action.debug.stepOut",
                "when": "inDebugMode"
            },
            {
                "mac": "f6",
                "win": "f6",
                "linux": "f6",
                "key": "f6",
                "command": "workbench.action.debug.stepOver",
                "when": "inDebugMode"
            },
            {
                "mac": "ctrl+f2",
                "win": "ctrl+f2",
                "linux": "ctrl+f2",
                "key": "ctrl+f2",
                "command": "workbench.action.debug.stop",
                "when": "inDebugMode"
            },
            {
                "key": "ctrl+f",
                "command": "actions.find"
            },
            {
                "key": "ctrl+s",
                "command": "workbench.action.files.save"
            },
            {
                "key": "shift+enter",
                "command": "editor.action.insertLineAfter",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "shift+ctrl+enter",
                "command": "editor.action.insertLineBefore",
                "when": "editorTextFocus && !editorReadonly"
            },
            {
                "key": "ctrl+x",
                "command": "execCut"
            },
            {
                "key": "ctrl+x",
                "command": "filesExplorer.cut",
                "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
            },
            {
                "key": "ctrl+x",
                "command": "editor.action.webvieweditor.cut",
                "when": "!editorFocus && !inputFocus && activeEditor == 'WebviewEditor'"
            },
            {
                "key": "ctrl+x",
                "command": "editor.action.clipboardCutAction",
                "when": "textInputFocus && !editorReadonly"
            },
            {
                "key": "ctrl+enter",
                "command": "workbench.action.submitComment",
                "when": "commentEditorFocused"
            },
            {
                "key": "ctrl+enter",
                "command": "scm.acceptInput",
                "when": "scmRepository"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^1.0.0"
    }
}
